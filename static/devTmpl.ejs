<% devs.forEach((dev) => { /* up, charge, devid, label */ %>
<div class="uk-card uk-card-default uk-card-hover uk-margin" id="device-<%= dev.devid %>-block">
    <div class="uk-grid-small" uk-grid>
        <div class="uk-width-1-3">
            <div class="uk-card">
                <div class="uk-card-header uk-padding-small">
                    <div class="uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-expand"><h4 class="dev-label"></h4></div>
                        <div class="uk-width-auto dev-status"></div>
                    </div>
                </div>

                <div class="uk-card-media-top">
                    <div class="uk-inline-clip uk-transition-toggle" style="overflow: visible;">
                        <img src="" class="dev-img">
                        <div id="atopimg"></div>
                    </div>
                </div>

                <div class="uk-card-footer uk-padding-small">
                    <div class="uk-grid-small uk-text-center uk-text-small" uk-grid>
                        <div class="uk-width-1-2" title="Температура воздуха">
                            <img src="/static/img/term.png">
                            <span class="dev-sens-temp"></span> &deg;C
                        </div>
                        <div class="uk-width-1-2" title="Атмосферное давление">
                            <img src="/static/img/press.png">
                            <span class="dev-sens-press"></span> мм.
                        </div>
                        <div class="uk-width-1-1 uk-text-meta" title="Дата фото и показаний">
                            <span class="dev-sens-date"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="uk-width-2-3">
            <div id="chartdiv-<%= dev.devid %>" class="chartdiv"></div>
        </div>
    </div>
</div>
<% }) %>